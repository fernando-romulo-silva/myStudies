		<dependency>
			<groupId>br.com.fernando</groupId>
			<artifactId>embedded-hsqldb-java-7</artifactId>
			<version>1.0.0</version>
			<scope>test</scope>
			<exclusions>
				<exclusion>
					<artifactId>guava</artifactId>
