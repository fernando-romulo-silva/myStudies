				</exclusion>
			</exclusions>
		</dependency>

		<!-- EMBEDDED GLASSFISH -->
 		<dependency>
			<groupId>br.com.fernando</groupId>
			<artifactId>embedded-glassfish-java-7</artifactId>
			<version>1.0.0</version>
			<scope>test</scope>
		</dependency> 
	
		<dependency>
			<groupId>br.com.caelum.stella</groupId>
			<artifactId>caelum-stella-core</artifactId>
			<version>1.2</version>
		</dependency>		
		
